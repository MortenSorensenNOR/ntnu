   "source": [
    "def f(t, k, a, x0):\n",
    "    return (k*x0*np.exp(a*t))/(k-x0+x0*np.exp(a*t))\n",
    "\n",
    "k = 100\n",
    "a = 1\n",
    "x0 = 100\n",
    "\n",
    "t = np.linspace(0, 10, 100)\n",
    "x = f(t, k, a, x0)\n",
    "\n",
    "plt.plot(t, x)\n",
    "plt.show()"
   ]
  }
